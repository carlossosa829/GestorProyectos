<div class="row d-flex justify-content-around mb-5">
    <% projects.forEach(function(project){ %>
        <div class="col-lg-3 col-md-5 col-sm-11 shadow m-3 d-flex flex-column justify-content-between p-0 m-0">
            <img class="card-img-top" src="/public/img/codigo.jpg" alt="Card image cap">
            <div class="card-body d-flex flex-column justify-content-between">
                <h5 class="card-title"><%= project.nombre_proyecto %></h5>
                <!-- <p class="card-text"><strong>Responsable: </strong> project.responsable </p> -->
                <p class="card-text"><strong>Descripci&oacute;n: </strong><%= project.descripcion %></p>
                <a href= "/profesor/details/<%= project.id_proyecto %>" class="btn btn-primary">Ver proyecto</a>
            </div>
        </div>
    <% }); %>
</div>
          